<Window x:Class="EasySave_Graphique.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        xmlns:language="clr-namespace:EasySave_Graphique.Properties.Langs"
        Title="MainWindow" Height="450" Width="800">

    <StackPanel Background="WhiteSmoke">
        <Frame x:Name="Main"/>
        <TabControl Height="435">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5">
                    <Label Content="Label" HorizontalAlignment="Left" Margin="238,156,0,0" VerticalAlignment="Top"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem"/>
            <Grid Background="#FFE5E5E5" Height="66" Width="794">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="16*"/>
                    <ColumnDefinition Width="79*"/>
                    <ColumnDefinition Width="699*"/>
                </Grid.ColumnDefinitions>
            </Grid>
        </TabControl>
    </StackPanel>
</Window>
