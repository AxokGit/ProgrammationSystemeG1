<Window x:Class="EasySave_Graphique.SelectLanguage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasySave_Graphique"
        mc:Ignorable="d"
        Title="SelectLanguage" Height="450" Width="800">
    <StackPanel Margin="20">
        <ComboBox Name="cmb" SelectionChanged="cmb_SelectionChanged">
            <ComboBoxItem Content="english"/>
            <ComboBoxItem Content="french"/>
        </ComboBox>
        <Button Content="load" Click="Button_Click"/>

        <Button Content="Get Button value" Click="Button_Click_1"/>
    </StackPanel>
</Window>
